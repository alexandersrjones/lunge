<!DOCTYPE html>
<html lang="en">

<head>
   <meta charset="utf-8" />

   <title>Lunge</title>
   {{> LoggedInHeader}}
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./Bootstrap/css/cosmo/bootstrap.min.css">
    <script src="./Bootstrap/js/jquery-2.1.3.min.js" type="text/javascript"></script>
    <script src="./Bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/style/regimePage.css">
    <script src="js/modal.js"></script>
    <script src="js/csvDownload.js"></script>
</head>

<body>

  <div class="background">
  </div>

    

  <div  class="wrapper">

    <div class="container innerwrap">

      <div class="shell">

            <div>
                <form action="/account" method="get">
                    <input type="submit" value="Edit" />
                </form>
            </div>

            <div id="sellerProfile">
                
                

                <div id="regimes">
                    <img id="profilePic" src="imgs/file_image.png" height="200" width="200">
                </div>
                


                <div id="nameDescrip">
                    <p>{{name}}</p>
                    <p>{{description}}</p>
                </div>
            </div>   

            <a href='#' onclick='downloadCSV({ filename: "regime.csv" });'>Export as CSV</a>
              
            <div id="structure">
                <div id="lists">
                    Other Regimes
                    <div id="regimes">
                        <img src="imgs/file_image.png" height="100" width="100">
                        <img src="imgs/file_image.png" height="100" width="100">
                        <img src="imgs/file_image.png" height="100" width="100">
                    </div>
                    
                    
                </div>

                <div id="feedback">
                    Feedback  
                </div>
            </div>

              
                 
     
          </div>
      </div>

      <br />

      <div class="footer">

        <footer>
          <div class="container-fluid">

            <div class="row">

            </div>
          </div>
        </footer>
      </div>
    </div>

</body>

</html>
